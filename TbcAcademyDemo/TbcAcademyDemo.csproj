    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>fe82f1c9-d4bf-488c-9f4e-797da87f7216</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
